--main

function _init()
	init_start_screen()
	if state=="game_screen" then
		init_game_screen()
	end
end

function _update()
 if state=="start_screen" then
 	update_start_screen()
 end
 if state=="game_screen" then
 	update_game_screen()
 
 end
 if state=="end_screen" then
 	update_end_screen()
 end
	
end

function _draw()
	if state=="start_screen" then
 	draw_start_screen()
 end
 if state=="game_screen" then
 	draw_game_screen()
 end
 if state=="end_screen" then
 	draw_end_screen()
 end
end
