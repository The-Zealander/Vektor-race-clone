--baur/player

baur = {
 x = 60,
 y = 60,
 speed = 1,
 walk = 1,
 sprint = 2,
 direction = "right",
 hp = 5,
 max_stamina = 10,
 stamina = 10,
 stamina_regen = 5 / 30,
 stamina_loss = 10 / 30,
 firebolt_timer = 0,
}

function init_baur()
 baur.x = 16
 baur.y = 16
 baur.speed = baur.walk
 baur.stamina = baur.max_stamina
end

function update_baur()
 local new_x, new_y = baur.x, baur.y
 if btn(â¬…ï¸) then
  new_x -= baur.speed
  baur.direction = "left"
 end
 if btn(âž¡ï¸) then
  new_x += baur.speed
  baur.direction = "right"
 end
 if not is_wall_tile_area(new_x, baur.y) then
  baur.x = new_x
 end
 if btn(â¬†ï¸) then
  new_y -= baur.speed
  baur.direction = "up"
 end
 if btn(â¬‡ï¸) then
  new_y += baur.speed
  baur.direction = "down"
 end
 if not is_wall_tile_area(baur.x, new_y) then
  baur.y = new_y
 end
 if btn(ðŸ…¾ï¸) then
  if baur.stamina > 4 then
   baur.speed = baur.sprint
   baur.stamina -= baur.stamina_loss
   if baur.stamina < 0 then
    baur.stamina = 0
   end
  elseif baur.stamina > 0 then
   baur.stamina -= baur.stamina_loss
   if baur.stamina < 0 then
    baur.stamina = 0
    baur.speed = baur.walk
   end
  end
 else
  baur.speed = baur.walk
 end
 if baur.speed == baur.walk then
  if baur.stamina <= 10 then
   baur.stamina += baur.stamina_regen
   if baur.stamina > baur.max_stamina then
    baur.stamina = baur.max_stamina
   end
  elseif baur.stamina > 0 then
   baur.stamina += baur.stamina_regen
   if baur.stamina > baur.max_stamina then
    baur.stamina = baur.max_stamina
   end
  end
 end
end

function draw_baur()

 if baur.direction == "up" then
  spr(1, baur.x, baur.y)
 elseif baur.direction == "down" then
  spr(3, baur.x, baur.y)
 elseif baur.direction == "left" then
  spr(2, baur.x, baur.y)
 elseif baur.direction == "right" then
  spr(4, baur.x, baur.y)
 end
end
