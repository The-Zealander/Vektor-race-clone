-- player/player
player = {
  x = 0,
  y = 0,
  speed = 0, -- current speed
  max_kph = 300, -- max speed in kph
  max_speed = 10, -- max speed in game units
  accel_kph = 10, -- acceleration in kph per second
  decel_kph = 5, -- deceleration in kph per second
  friction = 0.95, -- friction factor for natural slowdown
  angle = 0, -- angle in degrees
  sprite = 5, -- initial sprite
}

function init_player()
  player.x = 16
  player.y = 16
  player.speed = 0
  player.angle = 0 
  player.sprite = 5 -- facing right initially
end

function rotation()
  -- normalize the angle to be within 0 to 360 degrees
 if player.angle > 360 then
  player.angle = player.angle-360
 end
 if player.angle < 0 then
  player.angle = player.angle+360
 end
  if player.angle >= 337.5 or player.angle < 22.5 then
    player.sprite = 1 -- facing right
  elseif player.angle >= 22.5 and player.angle < 67.5 then
    player.sprite = 2 -- facing up-right
  elseif player.angle >= 67.5 and player.angle < 112.5 then
    player.sprite = 3 -- facing up
  elseif player.angle >= 112.5 and player.angle < 157.5 then
    player.sprite = 4 -- facing up-left
  elseif player.angle >= 157.5 and player.angle < 202.5 then
    player.sprite = 5 -- facing left
  elseif player.angle >= 202.5 and player.angle < 247.5 then
    player.sprite = 6 -- facing down-left
  elseif player.angle >= 247.5 and player.angle < 292.5 then
    player.sprite = 7 -- facing down
  elseif player.angle >= 292.5 and player.angle < 337.5 then
    player.sprite = 8 -- facing down-right
  end
end

-- new function: check if the player touches a tile with flag 7
function check_restart()
  -- get the tile at the player's current position
  local tile_x = flr(player.x / 8) -- convert position to tile index
  local tile_y = flr(player.y / 8)

  -- get the tile id at the player's position
  local tile_id = mget(tile_x, tile_y)

  -- new: check if the tile has flag 7 set
  if fget(tile_id, 7) then
    restart_game() -- restart the game if flag 7 is set
  end
end

-- new function: restart the game by resetting the player's state
function restart_game()
  -- reset player position and other state
  init_player() -- reinitializes the player to the starting position
  -- you can reset other variables here if needed
end

function update_player()
  -- convert max speed in kph to game units
  local max_game_speed = player.max_kph / 170 -- scale factor
  local accel = player.accel_kph / 170
  local decel = player.decel_kph / 170

  -- handle turning
  if btn(‚¨ÖÔ∏è) then
    player.angle = (player.angle - 1) % 360 -- turn right
  elseif btn(‚û°Ô∏è) then
    player.angle = (player.angle + 1) % 360 -- turn left
  end
 
  -- handle acceleration
  if btn(‚ùé) then
    player.speed = min(player.speed + accel, max_game_speed)
  elseif btn(üÖæÔ∏è) then
    player.speed = max(player.speed - decel, 0)
  else
    -- apply friction when not accelerating
    player.speed *= player.friction
  end

  -- calculate velocity based on angle and speed
  player.dx = player.speed * cos(player.angle/57.3)
  player.dy = player.speed * sin(player.angle/57.3)

  -- update position based on velocity
  local new_x = player.x + player.dx
  local new_y = player.y + player.dy

  -- prevent movement into walls
  if not is_wall_tile_area(new_x, player.y) then
    player.x = new_x
  end
  if not is_wall_tile_area(player.x, new_y) then
    player.y = new_y
  end
  
  -- check if player touches a tile with flag 7
  check_restart()
  
  -- update the sprite based on the current angle
  rotation()
end

function draw_player()
 print(player.angle,10,10,7)
  -- draw the player sprite based on direction
  spr(player.sprite, player.x, player.y)
end
