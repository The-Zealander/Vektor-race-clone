--game_screen

function init_game_screen()
 init_baur()
 level_width = 1024
 level_height = 512
 cam_x=0
 cam_y=0
end

function is_wall_tile(x, y)
 local tile_x = flr(x / 8)
 local tile_y = flr(y / 8)
 local sprite = mget(tile_x, tile_y)
 return fget(sprite, 0) -- check if flag 0 is set on the tile
end 

function is_wall_tile_area(x, y)
 return is_wall_tile(x, y) 
 or is_wall_tile(x + 7, y) 
 or is_wall_tile(x, y + 7) 
 or is_wall_tile(x + 7, y + 7)
end

function checkcollision(entitya, entityb)
 return entitya.x < entityb.x + entityb.width and
 entitya.x + entitya.width > entityb.x and
 entitya.y < entityb.y + entityb.height and
 entitya.y + entitya.height > entityb.y
end


function update_game_screen()
 --simple cam
 cam_x=baur.x-64+4
 cam_y=baur.y-64+4
 if cam_x<0 then
  cam_x=0
 end
 if cam_x>1024-128 then
  cam_x=1024-128
 end
 if cam_y<0 then
  cam_y=0
 end
 if cam_y>512-128 then
  cam_y=512-128
 end
 camera(cam_x,cam_y)
 update_baur()
end

-- function to draw the ui
function draw_iui()
 if show_text_box then
  rectfill(10+cam_x, 10+cam_y, 118+cam_x, 50+cam_y, 6)  -- background of the text box
  rect(10+cam_x, 10+cam_y, 118+cam_x, 50+cam_y, 5)      -- border of the text box
  print(current_interaction_text, 12+cam_x, 12+cam_y, 0)  -- display the interaction text
  print("press âŽ to close", 12+cam_x, 36+cam_y, 0)  -- instruction to close the box
 end
 if show_text_box and btnp(âŽ) then
  show_text_box = false
  current_interaction_text = ""
 end
end

function draw_game_screen()
 cls(0)
 map()
 draw_baur()
 draw_ui()
 for f in all(firebolts) do
  draw_firebolt (f)
 end
 for e in all(enemies) do
  draw_enemy(e)
 end
 draw_iui()
end
